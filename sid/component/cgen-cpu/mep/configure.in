dnl Process this file with autoconf to produce a configure script.
AC_INIT(Makefile.in)
AC_CONFIG_AUX_DIR(../../../..)
AM_INIT_AUTOMAKE(sidcomp,0.1)

dnl automake support
AM_MAINTAINER_MODE
dnl enable automatic rebuilding of cgen-generated files
RH_CGEN_MAINT
AC_LIBTOOL_WIN32_DLL
AM_PROG_LIBTOOL
AC_PROG_CXX
AC_PROG_CXXCPP
AC_EXEEXT
AC_LANG_CPLUSPLUS

dnl building docs
RH_DOCBUILD_CHECK

dnl Outputs
AC_OUTPUT(Makefile)
