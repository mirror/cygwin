# Additional configure-time settings.

global prefix
set prefix "@prefix@"
set enable_shared "@enable_shared@"
set target_alias "@target@"

# pull out "--enable-targets"/"--target" settings
global sidtarget
set sidtarget(arm) @sidtarget_arm@
set sidtarget(x86) @sidtarget_x86@
set sidtarget(mips) @sidtarget_mips@
set sidtarget(m32r) @sidtarget_m32r@
set sidtarget(m68k) @sidtarget_m68k@
set sidtarget(mt) @sidtarget_mt@
set sidtarget(ppc) @sidtarget_ppc@
set sidtarget(xstormy16) @sidtarget_xstormy16@
